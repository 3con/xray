{
  "name": "xray",
  "version": "0.0.1",
  "description": "This is an experiment in using the fundamental concepts of the Teletype CRDT as Atom's core text-storage data structure. It's implemented in Rust.",
  "main": "target/release/xray_node",
  "scripts": {
    "install": "npm run build-release",
    "build": "node_modules/.bin/napi build",
    "build-release": "node_modules/.bin/napi build --release",
    "check": "node_modules/.bin/napi check"
  },
  "types": "xray.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atom/xray.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "napi": "../napi"
  },
  "bugs": {
    "url": "https://github.com/atom/xray/issues"
  },
  "homepage": "https://github.com/atom/xray#readme"
}
